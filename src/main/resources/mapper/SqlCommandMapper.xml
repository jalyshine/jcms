<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="cn.jaly.admin.dao.SqlCommandMapper">

    <select id="selectCommandWithOrder" parameterType="String" resultType="java.util.LinkedHashMap">
        ${value}
    </select>

    <update id="updateCommand" parameterType="String">
        ${value}
    </update>

    <insert id="insertCommand" parameterType="String">
        ${value}
    </insert>

    <insert id="deleteCommand" parameterType="String">
        ${value}
    </insert>

</mapper>